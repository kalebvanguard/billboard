
<br/>
<br/>
<%= form_with(model: song, local: true, class: "ui form") do |f| %>
  <div class="field">
    <%= f.label :artist %>
    <%= f.text_field :artist, required: true, placeholder: "artist" %>
  </div>
  <div class="field">
    <%= f.label :genre %>
    <%= f.text_field :genre, required: true, placeholder: "genre" %>
  </div>
  <div class="top100">
    <%= f.label :top100 %>
    <%= f.text_field :phone, placeholder: "top100" %>
  </div>
  <div class="top50">
    <%= f.label :top50 %>
    <%= f.text_field :top50, required: true, placeholder: "top50" %>
  </div>
  <br />
  <br />
  <%= f.submit nil, class: "ui button" %>
<% end %>